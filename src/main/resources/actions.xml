<?xml version="1.0"?>
<!DOCTYPE ACTIONS SYSTEM "actions.dtd">
<ACTIONS>
    <ACTION NAME="ensime">
        <CODE>
            com.codecommit.es.EnsimePlugin.initProject(view);
        </CODE>
    </ACTION>
    <ACTION NAME="ensime.kill">
        <CODE>
            com.codecommit.es.EnsimePlugin.stopProject(view);
        </CODE>
    </ACTION>
    <ACTION NAME="ensime.inspect-type">
        <CODE>
            com.codecommit.es.EnsimePlugin.determineType(view);
        </CODE>
    </ACTION>
    <ACTION NAME="ensime.jump-to-declaration">
        <CODE>
            com.codecommit.es.EnsimePlugin.jumpToDeclaration(view);
        </CODE>
    </ACTION>
    <ACTION NAME="ensime.expand-selection">
        <CODE>
            com.codecommit.es.EnsimePlugin.expandSelection(view, null);
        </CODE>
    </ACTION>
    <ACTION NAME="ensime.typecheck-file">
        <CODE>
            com.codecommit.es.EnsimePlugin.typecheckFile(view.getBuffer());
        </CODE>
    </ACTION>
    <ACTION NAME="ensime.suggest-imports">
        <CODE>
            com.codecommit.es.EnsimePlugin.suggestImports(view);
        </CODE>
    </ACTION>
    <ACTION NAME="ensime.symbol-search">
        <CODE>
            com.codecommit.es.EnsimePlugin.symbolSearch(view);
        </CODE>
    </ACTION>
    <ACTION NAME="ensime.organize-imports">
        <CODE>
            com.codecommit.es.EnsimePlugin.organizeImports(view);
        </CODE>
    </ACTION>
    <ACTION NAME="ensime.rename">
        <CODE>
            com.codecommit.es.EnsimePlugin.rename(view);
        </CODE>
    </ACTION>
</ACTIONS>
